{
  "name": "questionnaire",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "runProd": "cd client && yarn build && ncp ./build ../server/build && cd ../server && yarn runProd",
    "start": "npm-run-all -p -r -l start:server start:client",
    "start:server": "cd server && yarn start",
    "start:client": "cd client && yarn start",
    "postinstall": "cd server && yarn install && cd ../client && yarn install",
    "test:client": "cd client && yarn test:once",
    "test:server": "cd server && yarn test:once",
    "test": "npm-run-all -s -l test:server test:client"
  },
  "devDependencies": {
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.2"
  }
}
